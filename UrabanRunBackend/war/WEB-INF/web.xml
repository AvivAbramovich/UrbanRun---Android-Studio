<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">

	<servlet>
		<servlet-name>SendLocation</servlet-name>
		<servlet-class>com.team2.urbanrun.SendLocation</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SendLocation</servlet-name>
		<url-pattern>/sendlocation/*</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>CheckIfInvited</servlet-name>
		<servlet-class>com.team2.urbanrun.CheckIfInvited</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CheckIfInvited</servlet-name>
		<url-pattern>/CheckIfInvited/*</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>EndScores</servlet-name>
		<servlet-class>com.team2.urbanrun.EndScores</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EndScores</servlet-name>
		<url-pattern>/EndScores/*</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>InitZone</servlet-name>
		<servlet-class>com.team2.urbanrun.InitZoneServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>InitZone</servlet-name>
		<url-pattern>/initZone/*</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>GameStart</servlet-name>
		<servlet-class>com.team2.urbanrun.GameStartServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GameStart</servlet-name>
		<url-pattern>/GameStart/*</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>DeclineInvitation</servlet-name>
		<servlet-class>com.team2.urbanrun.DeclineInvitation</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeclineInvitation</servlet-name>
		<url-pattern>/DeclineInvitation/*</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>GetGameProperties</servlet-name>
		<servlet-class>com.team2.urbanrun.GetGameProperties</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetGameProperties</servlet-name>
		<url-pattern>/GetGameProperties/*</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>getUsers</servlet-name>
		<servlet-class>com.team2.urbanrun.getUsers</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>getUsers</servlet-name>
		<url-pattern>/getUsers/*</url-pattern>
	</servlet-mapping>
	<!-- 
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list> -->
</web-app>
